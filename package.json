    "@daipendency/core-linux-arm64-gnu": "1.0.0",
    "@daipendency/core-linux-arm64-musl": "1.0.0",
    "@daipendency/core-win32-arm64-msvc": "1.0.0",
    "@daipendency/core-linux-arm-gnueabihf": "1.0.0",
    "@daipendency/core-linux-arm-musleabihf": "1.0.0",
    "@daipendency/core-linux-x64-musl": "1.0.0",
    "@daipendency/core-freebsd-x64": "1.0.0",
